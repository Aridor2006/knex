{
  "name": "knex",
  "version": "0.6.3",
  "description": "A batteries-included SQL query & schema builder for Postgres, MySQL and SQLite3 and the Browser",
  "main": "knex.js",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "minimist": "~0.0.9",
    "through": "^2.3.4",
    "browserify": "^4.0.0",
    "gulp": "^3.6.2",
    "gulp-bump": "^0.1.8",
    "gulp-git": "^0.4.2",
    "gulp-shell": "^0.2.5",
    "chai": "^1.9.1",
    "chai-as-promised": "^4.1.0",
    "jshint": "^2.5.0",
    "mocha": "^1.20.0",
    "mysql": "^2.3.1",
    "node-uuid": "~1.4.0",
    "pg": "^3.2.0",
    "pg-query-stream": "^0.6.0",
    "rimraf": "2.x",
    "sinon": "^1.10.0",
    "sinon-chai": "^2.5.0",
    "sqlite3": "^2.2.3",
    "mariasql": "~0.1.21",
    "oracle": "~0.3.7"
  },
  "dependencies": {
    "bluebird": "1.2.x",
    "colors": "~0.6.2",
    "generic-pool-redux": "~0.1.0",
    "inherits": "~2.0.1",
    "lodash": "~2.4.0",
    "mkdirp": "^0.5.0",
    "readable-stream": "^1.1.12",
    "knex-cli": "0.1.x"
  },
  "scripts": {
    "build": "gulp build",
    "test": "mocha -b --check-leaks -R spec test/index.js",
    "jshint": "jshint knex.js lib/*",
    "release:patch": "npm run jshint && npm test && gulp build && gulp bump --type patch && gulp release",
    "release:minor": "npm run jshint && npm test && gulp build && gulp bump --type minor && gulp release"
  },
  "bin": {
    "knex": "./node_modules/knex-cli/bin/knex-cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tgriesser/knex.git"
  },
  "keywords": [
    "sql",
    "query",
    "postgresql",
    "mysql",
    "sqlite3"
  ],
  "author": {
    "name": "Tim Griesser",
    "web": "https://github.com/tgriesser"
  },
  "license": "MIT"
}
